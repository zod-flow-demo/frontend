{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint --fix src/**/*.{ts,js,json,md}",
    "format": "prettier --write src/**/*.{ts,js,json,md}",
    "stylelint": "stylelint src/**/*.{css,scss} --fix",
    "mock": "env-cmd -f ./.env x-var msw-auto-mock ./meta/api.yaml -o ./src/mocks/handlers.ts -z ./src/api/api.client.ts --base-url $VITE_API_URL",
    "gen-zod-client": "openapi-zod-client ./meta/api.yaml -t ./src/api/template.hbs -o ./src/api/api.client.ts",
    "prepare": "husky install",
    "postinstall": "npm run mock && npm run gen-zod-client"
  },
  "devDependencies": {
    "@anatine/zod-mock": "^3.13.1",
    "@faker-js/faker": "^8.0.2",
    "@sveltejs/vite-plugin-svelte": "^2.4.3",
    "@tsconfig/svelte": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@zodios/core": "^10.9.2",
    "autoprefixer": "^10.4.14",
    "env-cmd": "^10.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte": "^2.32.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "msw": "^1.2.3",
    "msw-auto-mock": "github:LaiJunBin/msw-auto-mock",
    "mswx": "^0.4.1",
    "openapi-zod-client": "^1.9.0",
    "postcss": "^8.4.27",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "stylelint": "^15.10.2",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^3.0.0",
    "svelte": "^4.1.1",
    "svelte-check": "^3.4.6",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "x-var": "^0.0.1",
    "zod": "^3.21.4"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
